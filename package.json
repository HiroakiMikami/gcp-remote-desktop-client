  },
  "homepage": "https://github.com/HiroakiMikami/cloud-linux-desktop-client#readme",
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/commander": "^2.12.2",
    "@types/mocha": "^5.2.0",
    "chai": "^4.1.2",
    "mocha": "^5.1.1",
    "tslint": "^5.12.0",
    "typedoc": "^0.11.1",
    "typemoq": "^2.1.0",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "commander": "^2.19.0"
  }
